<%= form_for @attraction do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %> <br>
  <%= f.label :nausea %>
  <%= f.number_field :nausea_rating, min: 1, max: 5 %> <br>
  <%= f.label :happiness %>
  <%= f.number_field :happiness_rating, min: 1, max: 5 %> <br>
  <%= f.label :tickets %>
  <%= f.number_field :tickets, min: 1, max: 5 %> <br>
  <%= f.label :height %>
  <%= f.number_field :min_height %> <br>
  <%= f.submit %>
<% end %>
